@model IEnumerable<DocumentManagementSystem.Models.ViewModels.Books4.IndexAddition.Item>

@if (Model.Count() > 0)
{
    <hr />

    foreach (var item in Model)
    {
        if (!item._isExist)
        {
            <dt>
                <span>错误</span>
            </dt>
            <dd>
                <span>错误</span>
            </dd>
        }
        else
        {
            <dt>
                <span>@item.Name</span>
            </dt>
            <dd>
                <span>@item.Description</span>
            </dd>
        }
    }
}