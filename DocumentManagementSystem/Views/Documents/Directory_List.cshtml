@model IEnumerable<DocumentManagementSystem.Models.ViewModels.Documents.Directory.Item>

<table class="table table-bordered table-hover table-condensed">
    <thead>
        <tr>
            <th class="text-center">
                @Html.DisplayNameFor(model => model.NodeName)
            </th>
            <th class="text-center">
                @Html.DisplayNameFor(model => model.IsFinished)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.PreSpace)
                    @Html.ActionLink(item.NodeName, "Details", new { id = item.DocumentId }, new { target = "_blank" })                   
                </td>
                <td class="text-center" width="10%">
                    @Html.DisplayFor(modelItem => item.IsFinished)
                </td>
            </tr>
        }
    </tbody>
</table>